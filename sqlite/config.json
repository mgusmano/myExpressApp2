{
	"database": {
		"dialect": "sqlite",
		"storage": ".//sqlite//db//sqlite.db",
		"logging": false,
		"dialectOptions": {
			"socketPath": "/tmp/mysql.sock",
			"supportBigNumbers": true,
			"bigNumberStrings": true
		},
		"define": {
			"createdAt": "created",
			"updatedAt": "updated",
			"deletedAt": "deleted",
			"underscored": true
		}
	}
}
